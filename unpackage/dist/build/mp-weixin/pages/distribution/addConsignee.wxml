<view><header vue-id="5f3f2af4-1" title="{{address_id?'編輯收貨人資訊':'新增收貨人資訊'}}" image="{{address_id?'../../static/img/shangchu.png':''}}" data-event-opts="{{[['^gotoF',[['gotoF']]]]}}" bind:gotoF="__e" bind:__l="__l"></header><view class="addList"><view class="addItem"><view class="title">收貨人</view><input type="text" placeholder="請輸入收貨人姓名" data-event-opts="{{[['input',[['__set_model',['','name','$event',[]]]]]]}}" value="{{name}}" bindinput="__e"/></view><view class="addItem"><view class="title">收貨地址</view><view class="city"><picker mode="selector" range="{{arealist}}" range-key="name" value="{{index}}" data-event-opts="{{[['change',[['changeCity',['$event']]]]]}}" bindchange="__e"><view>{{(arealist.length!=0?arealist[index].name:'市')+''}}<image src="../../static/img/xiala.png" mode="widthFix"></image></view></picker><picker mode="selector" range="{{itemslist}}" range-key="name" value="{{index1}}" data-event-opts="{{[['change',[['changeCountry',['$event']]]]]}}" bindchange="__e"><view>{{(itemslist.length!=0?itemslist[index1].name:'區')+''}}<image src="../../static/img/xiala.png" mode="widthFix"></image></view></picker></view><input type="text" placeholder="詳細地址(里道路/街/巷弄號)" data-event-opts="{{[['input',[['__set_model',['','address','$event',[]]]]]]}}" value="{{address}}" bindinput="__e"/></view><view class="addItem"><view class="title">聯繫電話</view><input type="text" placeholder="請輸入手機號碼" data-event-opts="{{[['input',[['__set_model',['','telephone','$event',[]]]]]]}}" value="{{telephone}}" bindinput="__e"/></view><view class="footer"><navigator url data-event-opts="{{[['tap',[['keep']]]]}}" bindtap="__e">儲存</navigator><view hidden="{{!(showTabBar===true)}}" class="tab-bar"></view></view></view></view>