.no {
  text-align: center;
  color: #999;
  margin-top: 200rpx;
}
.tab-bar {
  height: 30rpx;
  background-color: #FF9EC1;
}
.useritem {
  padding: 10rpx 60rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  background: #F3F3F3;
  margin-bottom: 3px;
}
.useritem image:first-child {
  width: 46rpx;
  height: 46rpx;
}
.useritem image:last-child {
  width: 30rpx;
  height: 30rpx;
}
.useritem .center {
  display: flex;
  flex-direction: column;
  flex: 1;
  padding-left: 48rpx;
}
.useritem .center text {
  font-size: 30rpx;
  margin-bottom: 10rpx;
  color: #707070;
}
.useritem .center text:first-child {
  font-size: 34rpx;
  color: #000;
  font-weight: 700;
}
.footer {
  position: fixed;
  bottom: 0;
  width: 100%;
  text-align: center;
}
.footer navigator {
  padding: 20rpx 0;
  background-color: #FF9EC3;
  color: #fff;
}

