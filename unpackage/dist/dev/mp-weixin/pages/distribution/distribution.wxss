.model picker {
  width: 100%;
  box-sizing: border-box;
}
.model picker view {
  display: flex;
  justify-content: space-between;
}
.radiogroup {
  margin-bottom: 20rpx;
  border: 1px solid #A3A3A3;
  border-radius: 20rpx;
  background-color: #fff;
}
.radiogroup .lable {
  display: flex;
  padding: 20rpx;
  align-items: center;
}
.radiogroup .lable:last-child {
  border-bottom: none;
}
.pir > picker {
  width: 100%;
  box-sizing: border-box;
}
.pir > picker view {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.number {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin: 30rpx 0;
}
.number text {
  display: inline-block;
  width: 80rpx;
  height: 80rpx;
  text-align: center;
  line-height: 80rpx;
  font-size: 50rpx;
  color: #505050;
}
.number .add {
  border: 1px solid #A3A3A3;
  border-radius: 15rpx;
  background-color: #fff;
}
.number .hs {
  background-color: #eaeaea;
}
.wuliu {
  display: flex;
  justify-content: flex-end;
  color: red;
  font-size: 26rpx;
  margin: 50rpx 0;
}
.body {
  background-color: #F3F3F3;
}
.body picker {
  border: 1px solid #A3A3A3;
  padding: 20rpx 38rpx;
  border-radius: 15rpx;
  display: inline-block;
  margin: 0 10rpx 10rpx 0;
  background-color: #fff;
}
.body picker image {
  margin-left: 10rpx;
  width: 40rpx;
  height: 40rpx;
  vertical-align: middle;
}
.body .head {
  font-size: 40rpx;
  padding-left: 20rpx;
  color: #505050;
  margin-bottom: 8rpx;
}
.body .header {
  padding: 0 200rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 40rpx;
  color: #A1A1A1;
  background-color: #fff;
}
.body .header text {
  padding: 16rpx 0;
}
.body .header .active {
  color: #505050;
  border-bottom: 6rpx solid #FF85B4;
}
.body .exchange {
  text-align: center;
}
.body .exchange navigator {
  display: inline-block;
  padding: 16rpx 250rpx;
  border: 4rpx solid #FF85B4;
  border-radius: 50rpx;
  background-color: #fff;
  color: #FF85B4;
  font-size: 42rpx;
  font-weight: 600;
  letter-spacing: 4rpx;
  margin-top: 36rpx;
}
.body .exchange .ture {
  color: #fff;
  background-color: #FF85B4;
}
.body .consulting {
  box-sizing: border-box;
  padding: 40rpx 32rpx;
  border-bottom: 6rpx solid #fff;
}
.body .consulting .title {
  font-size: 40rpx;
  font-weight: 600;
  letter-spacing: 8rpx;
  margin-bottom: 36rpx;
  text-align: center;
}
.body .consulting .contacts {
  display: flex;
  font-size: 26rpx;
  justify-content: center;
  padding-bottom: 4rpx;
  letter-spacing: 4rpx;
  border-bottom: 2rpx solid #A3A3A3;
  margin-bottom: 36rpx;
}
.body .consulting .contacts checkbox-group {
  font-weight: 600;
}
.body .consulting .contacts navigator {
  padding-top: 4rpx;
  color: #66A4B3;
  text-decoration: underline;
}
.body .consulting .time .stime {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 14rpx;
  border: 1px solid #A3A3A3;
  padding: 18rpx 36rpx;
  border-radius: 20rpx;
  font-size: 32rpx;
  background-color: #fff;
}
.body .consulting .time .stime image {
  width: 16rpx;
  height: 16rpx;
}
.body .consulting .item {
  margin-bottom: 30rpx;
  width: 100%;
}
.body .consulting .item .head {
  font-size: 40rpx;
  padding-left: 20rpx;
  color: #505050;
  margin-bottom: 8rpx;
}
.body .consulting .item > input {
  margin-bottom: 14rpx;
  border: 1px solid #A3A3A3;
  padding: 18rpx 36rpx;
  border-radius: 20rpx;
  font-size: 36rpx;
  background-color: #fff;
}
.body .consulting .item textarea {
  box-sizing: border-box;
  width: 100%;
  margin-bottom: 14rpx;
  border: 1px solid #A3A3A3;
  padding: 18rpx 36rpx;
  border-radius: 20rpx;
  font-size: 42rpx;
  background-color: #fff;
}
.body .consulting .city {
  display: flex;
}
.body .consulting .city .uni-combox {
  margin-bottom: 14rpx;
  border: 1px solid #A3A3A3;
  padding-left: 36rpx;
  border-radius: 20rpx;
  background-color: #fff;
  margin-right: 40rpx;
}
.body .consulting .city input {
  width: 100rpx;
}
.body .coupon {
  padding: 38rpx 58rpx;
  letter-spacing: 4rpx;
  border-bottom: 6rpx solid #fff;
}
.body .coupon .coupon_na {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.body .coupon .coupon_na > image {
  width: 44rpx;
  height: 44rpx;
  margin-right: 14rpx;
}
.body .coupon .coupon_na > text {
  font-size: 40rpx;
  font-weight: 600;
  flex: 1;
}
.body .coupon .coupon_na .right {
  display: flex;
  align-items: center;
}
.body .coupon .coupon_na .right text {
  font-size: 42rpx;
  color: #D8D8D8;
}
.body .coupon .coupon_na .right image {
  width: 40rpx;
  height: 40rpx;
}
.body .tile {
  padding: 38rpx 58rpx;
  letter-spacing: 4rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.body .tile .img {
  background-color: #FF85B4;
  border-radius: 50%;
  width: 44rpx;
  height: 44rpx;
  margin-right: 14rpx;
}
.body .tile .img image {
  width: 44rpx;
  height: 44rpx;
}
.body .tile > text {
  font-size: 40rpx;
  font-weight: 600;
  flex: 1;
}
.body .tile .right {
  display: flex;
  align-items: center;
}
.body .tile .right text {
  font-size: 42rpx;
  color: #FF85B4;
}
.body .tile .right image {
  width: 100rpx;
  height: 100rpx;
}
.body .button {
  padding: 28rpx;
  background-color: #fff;
}
.body .button button {
  border-radius: 50rpx;
  background-color: #FF85B4;
  color: #fff;
  font-size: 40rpx;
  letter-spacing: 6rpx;
  font-weight: 600;
  box-shadow: 0rpx 7rpx 10rpx 0rpx rgba(0, 0, 0, 0.2);
}

