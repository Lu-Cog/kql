.tab-bar {
  height: 30rpx;
  background-color: #FF9EC1;
}
.addList {
  padding: 32rpx;
}
.addList .addItem {
  margin-bottom: 10rpx;
}
.addList .addItem .title {
  color: #505050;
  font-size: 40rpx;
  padding: 14rpx;
}
.addList picker {
  border: 1px solid #A3A3A3;
  padding: 20rpx 38rpx;
  border-radius: 15rpx;
  display: inline-block;
  margin: 0 10rpx 10rpx 0;
}
.addList picker image {
  margin-left: 10rpx;
  width: 40rpx;
  height: 40rpx;
  vertical-align: middle;
}
.addList input {
  border: 1px solid #A3A3A3;
  border-radius: 15rpx;
  padding: 20rpx 38rpx;
}
.footer {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  text-align: center;
}
.footer navigator {
  padding: 20rpx 0;
  background-color: #FF9EC3;
  color: #fff;
  font-weight: 700;
}

