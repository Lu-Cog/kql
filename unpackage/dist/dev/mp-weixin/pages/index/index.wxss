.radiogroup {
  padding: 10rpx;
  width: 90%;
  min-height: 500rpx;
  max-height: 1000rpx;
  overflow: auto;
  background-color: #fff;
}
.radiogroup .lable {
  display: flex;
  margin-bottom: 20rpx;
  box-shadow: 0 0 2rpx 4rpx rgba(0, 0, 0, 0.07);
  border-radius: 20rpx;
  padding: 20rpx;
  align-items: center;
}
.popupMain1 {
  width: 640rpx;
  background-color: #fff;
  box-sizing: border-box;
  border-radius: 20rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex-direction: column;
  position: relative;
}
.popupMain1 .ptitle {
  position: relative;
  top: 0;
  width: 100%;
  padding: 20rpx;
  text-align: center;
  box-sizing: border-box;
  box-shadow: 4rpx 0 4rpx 4rpx rgba(0, 0, 0, 0.07);
}
.popupMain1 .btn {
  position: relative;
  bottom: 0;
  box-shadow: -4rpx 0 4rpx 4rpx rgba(0, 0, 0, 0.07);
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.popupMain1 .btn .btnItem {
  flex: 1;
  text-align: center;
  padding: 20rpx;
  box-sizing: border-box;
}
.popupMain1 .btn .btnItem:last-child {
  border-left: 1rpx solid #eee;
}
.popupMain {
  width: 540rpx;
  height: 766rpx;
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  margin-bottom: 100rpx;
}
.popupMain .closeP {
  position: absolute;
  top: 15rpx;
  right: 15rpx;
  width: 50rpx;
  height: 50rpx;
  color: #666;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #fff;
}
.popupMain .bgImgP {
  position: absolute;
  z-index: -1;
  width: 100%;
  border-radius: 30rpx;
}
.popupMain .ptitle {
  margin-top: 270rpx;
  font-size: 38rpx;
  margin-bottom: 10rpx;
  font-weight: 600;
  color: #fff;
}
.popupMain .vipMain {
  background-color: #fff;
  width: 90%;
  border-radius: 30rpx;
  padding: 10rpx;
  box-sizing: border-box;
}
.popupMain .vipMain > ._p {
  text-align: center;
  font-size: 30rpx;
  font-weight: 700;
  margin: 0rpx 0 10rpx;
}
.popupMain .vipMain .vipList {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.popupMain .vipMain .vipList .vipItem {
  padding: 10rpx;
  width: 48%;
  box-sizing: border-box;
  background-color: #F2F2F2;
  border-radius: 30rpx;
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
}
.popupMain .vipMain .vipList .vipItem ._p {
  display: flex;
  align-items: center;
  font-weight: 600;
  font-size: 26rpx;
  color: #FF85B4;
  white-space: nowrap;
}
.popupMain .vipMain .vipList .vipItem ._p:last-child {
  margin-top: 10rpx;
  font-size: 36rpx;
  color: #FF9EC3;
}
.popupMain .vipMain .vipList .vipItem ._p:first-child {
  margin-bottom: 10rpx;
  color: #000;
}
.popupMain .vipMain .vipList .vipItem .bigText {
  font-size: 36rpx;
  color: #FF85B4 !important;
}
.popupMain .vipMain .vipList .vipItem .cenText {
  color: #000;
}
.popupMain .vipMain .vipList .vipItem .lastText {
  font-size: 30rpx !important;
  color: #FF9EC3;
  text-decoration: underline;
}
.popupMain .vipMain .vipList .vipItem image {
  width: 40rpx;
  height: 40rpx;
  background-color: #FF85B4;
  border-radius: 50%;
}
.popupMain .tip {
  width: 90%;
  text-align: center;
  font-size: 20rpx;
  color: #666;
  margin: 10rpx 0 0;
}
.popupMain .payTypeTip {
  width: 90%;
  text-align: center;
  font-size: 26rpx;
  color: #333;
  margin: 20rpx 0 20rpx;
}
.popupMain .payTypeTip text {
  background-color: rgba(255, 255, 255, 0.7);
  border-radius: 50rpx;
  padding: 4rpx 30rpx;
  color: #FF9EC3;
  text-decoration: underline;
}
.popupMain .join {
  height: 70rpx;
  width: 300rpx;
  margin-top: 40rpx;
  display: flex;
  font-size: 30rpx;
  justify-content: center;
  align-items: center;
  background-color: rgba(255, 255, 255, 0.7);
}
.popupMain .yvip {
  background-color: rgba(240, 240, 240, 0.7);
}
.mask2 {
  padding: 30rpx 50rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  background-color: #fff;
  z-index: 1000;
  box-shadow: -4rpx 0rpx 4rpx 4rpx rgba(0, 0, 0, 0.07);
  box-sizing: border-box;
}
.mask2 image {
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  right: 40rpx;
  top: 20rpx;
}
.mask2 .title {
  padding-top: 40rpx;
}
.mask2 .uni-rate {
  padding: 30rpx 0;
}
.mask {
  padding: 30rpx 50rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  background-color: #fff;
  z-index: 1000;
  box-shadow: -4rpx 0rpx 4rpx 4rpx rgba(0, 0, 0, 0.07);
  display: flex;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
}
.mask image {
  width: 22rpx;
  height: 22rpx;
}
.mask .top {
  color: #FF85B4;
  font-weight: 500;
  margin-bottom: 6rpx;
}
.mask .bottom {
  font-size: 30rpx;
}
.mask .right {
  font-weight: 700;
}
._header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 30rpx;
  color: #FF9EC3;
  padding: 20rpx;
}
._header image {
  width: 30rpx;
  height: 30rpx;
}
._header .left {
  text-align: center;
}
._header .right {
  margin: 0 20rpx;
  width: 50rpx;
}
._header .header_cen {
  display: flex;
  align-items: center;
  box-shadow: 0px 0rpx 10rpx 0rpx rgba(0, 0, 0, 0.1);
  font-size: 30rpx;
  color: #000;
  border-radius: 10rpx;
  margin: 0 20rpx;
  padding: 14rpx 28rpx;
  flex: 1;
}
._header .header_cen image {
  width: 40rpx;
  height: 40rpx;
  margin-right: 20rpx;
}
._header .header_cen .article {
  flex: 1;
}
._header .header_cen .article swiper {
  height: 20px !important;
  line-height: 20px;
}
.main {
  width: 100%;
  text-align: center;
  margin-top: 80rpx;
}
.main image {
  width: 260rpx;
  height: 600rpx;
}
.main .mian_top {
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
  margin-bottom: 10rpx;
}
.main .mian_top navigator {
  background: #FF85B4;
  color: #fff;
  font-size: 40rpx;
  font-weight: 600;
  letter-spacing: 4rpx;
  border-radius: 80rpx;
  padding: 20rpx 150rpx;
  position: absolute;
}
.main .mian_top .real_time {
  bottom: 170rpx;
}
.main .mian_top .appointment {
  bottom: 50rpx;
}
.main .vip {
  color: #FF85B4;
  font-size: 36rpx;
  font-weight: 600;
  letter-spacing: 10rpx;
  margin-bottom: 24rpx;
}
.main .kg {
  font-size: 30rpx;
  letter-spacing: 5rpx;
  margin-bottom: 18rpx;
}
.main .yq {
  font-size: 30rpx;
  letter-spacing: 5rpx;
}
.banner {
  display: flex;
  justify-content: space-between;
  margin-top: 40rpx;
}
.banner swiper {
  width: 100%;
  padding: 30rpx;
  text-align: center;
}
.banner swiper image {
  width: 90%;
  height: 180rpx;
  border-radius: 20rpx;
}
.banner .left {
  text-align: center;
  line-height: 210rpx;
  background-color: #FFEBF2;
  width: 160rpx;
  height: 160rpx;
  border-bottom-right-radius: 16rpx;
  border-top-right-radius: 16rpx;
}
.banner .left image {
  width: 100rpx;
  height: 100rpx;
}
.banner .right {
  border-top-left-radius: 16rpx;
  border-bottom-left-radius: 16rpx;
  overflow: hidden;
}
.banner .right image {
  height: 160rpx;
  width: 540rpx;
}
.talk {
  position: absolute;
  right: 50rpx;
  bottom: 300rpx;
}
.talk image {
  width: 120rpx;
  height: 120rpx;
}

