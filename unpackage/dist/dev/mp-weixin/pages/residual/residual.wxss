.header {
  background: #FF9EC3;
  padding: 44rpx 30rpx 44rpx;
}
.header .img {
  text-align: center;
  position: relative;
}
.header .img image {
  width: 286rpx;
  height: 200rpx;
}
.header .img .right {
  position: absolute;
  right: 0;
  bottom: 20rpx;
}
.header .img .right navigator {
  background-color: #FF85B4;
  border-radius: 40rpx;
  padding: 4rpx 10rpx;
  font-size: 30rpx;
  letter-spacing: 4rpx;
  color: #fff;
  margin-bottom: 30rpx;
}
.header .img .right .nav {
  color: #FF85B4;
  background-color: #fff;
}
.header .img .right text {
  color: #fff;
  font-size: 50rpx;
}
.header .accumulation .title {
  font-size: 30rpx;
  letter-spacing: 4rpx;
  font-weight: 600;
  padding: 14rpx;
}
.header .accumulation .progress {
  display: flex;
  align-items: center;
  height: 54rpx;
  border: 4rpx solid #FF85B4;
  background-color: #fff;
  font-size: 30rpx;
  font-weight: 600;
}
.header .accumulation .progress > text {
  color: #FF85B4;
  padding-left: 16rpx;
}
.header .accumulation .progress .bar {
  height: 100%;
  width: 0%;
  background-color: #FF85B4;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.header .accumulation .progress .bar text {
  color: #fff;
  padding-right: 16rpx;
}
.definite {
  padding: 44rpx;
}
.definite .title {
  font-size: 36rpx;
  font-weight: 600;
  letter-spacing: 6rpx;
  margin-bottom: 20rpx;
}
.definite .content {
  width: 100%;
  height: 600rpx;
  background: #F3F3F3;
  border-radius: 20rpx;
  padding: 36rpx 22rpx;
  box-sizing: border-box;
  overflow: auto;
}
.definite .content .content_item {
  border-bottom: 1px solid #707070;
  margin-bottom: 34rpx;
}
.definite .content .content_item .item_bottom {
  display: flex;
  justify-content: space-between;
  padding-bottom: 8rpx;
  font-size: 30rpx;
  letter-spacing: 6rpx;
}

