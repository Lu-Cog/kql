.top_title {
  padding: 116rpx 0 74rpx;
  font-size: 70rpx;
  letter-spacing: 8rpx;
  font-weight: 600;
  text-align: center;
  color: #FF9EC3;
}
.wabi {
  display: flex;
  justify-content: center;
}
.wabi .img {
  width: 304rpx;
  height: 304rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 50%;
  background: #FF9EC3;
  box-shadow: 0rpx 7rpx 10rpx 0rpx rgba(0, 0, 0, 0.2);
}
.wabi .img image {
  width: 290rpx;
  height: 290rpx;
}
.num {
  text-align: center;
  margin-top: 30rpx;
  color: #FF9EC3;
  font-size: 50rpx;
  font-weight: 600;
}

