.header {
  position: relative;
  height: 160rpx;
  width: 100%;
  background-color: #FF9EC3;
}
.header .head {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 170rpx;
  height: 170rpx;
  position: absolute;
  left: 50%;
  bottom: -60rpx;
  -webkit-transform: translate(-50%);
          transform: translate(-50%);
  border-radius: 50%;
  background-color: #FF9EC3;
}
.header .head image {
  width: 160rpx;
  height: 160rpx;
}
.num {
  margin-top: 80rpx;
  text-align: center;
  font-size: 50rpx;
  font-weight: 600;
  margin-bottom: 20rpx;
}
.transfer {
  text-align: center;
}
.transfer navigator {
  display: inline-block;
  background: #FF9EC3;
  padding: 4rpx 30rpx;
  border-radius: 60rpx;
  color: #fff;
  letter-spacing: 6rpx;
  font-weight: 600;
}
.definite {
  padding: 44rpx;
}
.definite .title {
  font-size: 32rpx;
  font-weight: 600;
  letter-spacing: 6rpx;
  margin-bottom: 20rpx;
}
.definite .content {
  width: 100%;
  height: 700rpx;
  background: #F3F3F3;
  border-radius: 20rpx;
  padding: 36rpx 22rpx;
  box-sizing: border-box;
  overflow: auto;
}
.definite .content .content_item {
  border-bottom: 1px solid #707070;
  margin-bottom: 34rpx;
}
.definite .content .content_item .item_title {
  font-weight: 600;
  font-size: 34rpx;
  letter-spacing: 6rpx;
  padding-bottom: 10rpx;
}
.definite .content .content_item .item_bottom {
  display: flex;
  justify-content: space-between;
  padding-bottom: 8rpx;
  font-size: 30rpx;
  letter-spacing: 6rpx;
}

