.loginout {
  font-size: 30rpx;
  color: #fff;
  text-align: center;
  padding: 16rpx 20rpx;
  background-color: #FF85B4;
  border-radius: 10rpx;
  width: 80%;
  margin: 80rpx auto;
}
.user {
  height: 100vh;
  background-color: #F8F8F8;
}
.header {
  background-color: #FF9EC3;
}
.header .top {
  height: 25px;
  width: 100%;
}
.header .head {
  padding: 60rpx 24rpx 30rpx;
  align-items: center;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
}
.header .head .headImg {
  position: relative;
}
.header .head .headImg .vip {
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  z-index: 100;
  top: -20rpx;
  left: -10rpx;
}
.header .head image {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  flex-shrink: 0;
}
.header .head .center {
  flex: 1;
  color: #fff;
  padding: 0 0 0 20rpx;
  width: 50%;
}
.header .head .center .name {
  width: 100%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 40rpx;
  margin-bottom: 12rpx;
}
.header .head .center .vipDate {
  font-size: 24rpx;
  margin: 10rpx 0;
}
.header .head .img {
  font-size: 30rpx;
  display: flex;
  color: #fff;
}
.header .head .img .item {
  margin-right: 20rpx;
  display: flex;
  align-items: center;
}
.header .head .img .item image {
  margin-right: 4rpx;
}
.header .head .img .item text {
  white-space: nowrap;
}
.header .head .img image {
  width: 50rpx;
  height: 50rpx;
  vertical-align: middle;
}
.funlist .item {
  background-color: #fff;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 30rpx;
  margin-bottom: 1px;
}
.funlist .item image {
  width: 40rpx;
  height: 40rpx;
}
.funlist .item image:last-child {
  width: 16rpx;
  height: 16rpx;
}
.funlist .item text {
  flex: 1;
  padding-left: 16rpx;
  color: #6C6C6C;
  font-size: 34rpx;
}
.funlist .set {
  margin-top: 20rpx;
}

