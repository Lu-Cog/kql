.no {
  text-align: center;
  color: #999;
  margin-top: 200rpx;
}
.search {
  padding: 38rpx;
  position: relative;
}
.search input {
  box-shadow: 0 0 7rpx 4rpx rgba(0, 0, 0, 0.07);
  border-radius: 50rpx;
  padding: 14rpx 20rpx 14rpx 90rpx;
}
.search image {
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  top: 46rpx;
  left: 60rpx;
}
.user_item {
  margin: 0 50rpx 40rpx;
  display: flex;
  align-items: center;
}
.user_item image {
  width: 60rpx;
  height: 60rpx;
  margin-right: 10rpx;
}
.user_item .top {
  color: #6C6C6C;
  font-size: 32rpx;
}
.user_item .bottom {
  color: #6C6C6C;
  font-size: 30rpx;
}

